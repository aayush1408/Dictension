{
    "manifest_version":2,
    "name":"Word dictionary",
    "version":"0.001",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>" 
            ],
            "js":["content.js"]
        }
    ],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_icon":"dict.png",
        "default_popup":"sketch/index.html",
        "default_title":"Lookup a def!"
    },
    "icons": {
        "16": "dict.png",
        "48": "dict.png",
        "128": "dict.png"
    }
}